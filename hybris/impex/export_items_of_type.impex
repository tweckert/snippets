#
# Exports all items of a hybris item type.
#
# Usage: replace 'SomeItemType' by the hybris item type that should be exported,
# modify the 'INSERT' impex line to export the desired item attributes
#
"#% impex.setTargetFile( ""SomeItemType.csv"" );"
INSERT SomeItemType;code[allownull=true];name[lang=de];visible
"#% impex.exportItems( ""SomeItemType"" , false );"